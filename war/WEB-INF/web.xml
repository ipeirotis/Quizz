<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>AddRelation</servlet-name>
		<servlet-class>com.ipeirotis.adcrowdkg.AddRelation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddEntity</servlet-name>
		<servlet-class>com.ipeirotis.adcrowdkg.AddEntity</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddUserEntry</servlet-name>
		<servlet-class>com.ipeirotis.adcrowdkg.AddUserEntry</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>AddRelation</servlet-name>
		<url-pattern>/addRelation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddEntity</servlet-name>
		<url-pattern>/addEntity</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUserEntry</servlet-name>
		<url-pattern>/addUserEntry</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
