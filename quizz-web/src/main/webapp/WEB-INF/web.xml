<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

	<servlet>
		<servlet-name>AddQuiz</servlet-name>
		<servlet-class>us.quizz.servlets.AddQuiz</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>StartQuiz</servlet-name>
		<servlet-class>us.quizz.servlets.StartQuiz</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddQuestion</servlet-name>
		<servlet-class>us.quizz.servlets.AddQuestion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddTreatment</servlet-name>
		<servlet-class>us.quizz.servlets.AddTreatment</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddBadge</servlet-name>
		<servlet-class>us.quizz.servlets.AddBadge</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>AddUserAnswer</servlet-name>
		<servlet-class>us.quizz.servlets.AddUserAnswer</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>ProcessUserAnswer</servlet-name>
		<servlet-class>us.quizz.servlets.ProcessUserAnswer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DownloadUserAnswers</servlet-name>
		<servlet-class>us.quizz.servlets.DownloadUserAnswers</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetNumberOfSubmittedAnswers</servlet-name>
		<servlet-class>us.quizz.servlets.api.GetNumberOfSubmittedAnswers</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>DeleteQuiz</servlet-name>
		<servlet-class>us.quizz.servlets.DeleteQuiz</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>UpdateUserQuizStatistics</servlet-name>
		<servlet-class>us.quizz.servlets.api.UpdateUserQuizStatistics</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateUserExperiment</servlet-name>
		<servlet-class>us.quizz.servlets.api.UpdateUserExperiment</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FacebookLogin</servlet-name>
		<servlet-class>us.quizz.servlets.FacebookLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CampaignManagement</servlet-name>
		<servlet-class>us.quizz.ads.CampaignManagement</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StartQuiz</servlet-name>
		<url-pattern>/startQuiz</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddQuiz</servlet-name>
		<url-pattern>/addQuiz</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddQuestion</servlet-name>
		<url-pattern>/addQuestion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddUserAnswer</servlet-name>
		<url-pattern>/addUserAnswer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcessUserAnswer</servlet-name>
		<url-pattern>/processUserAnswer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DownloadUserAnswers</servlet-name>
		<url-pattern>/admin/downloadUserAnswers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddTreatment</servlet-name>
		<url-pattern>/addTreatment</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddBadge</servlet-name>
		<url-pattern>/addBadge</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetNumberOfSubmittedAnswers</servlet-name>
		<url-pattern>/api/getNumberOfSubmittedAnswers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateUserQuizStatistics</servlet-name>
		<url-pattern>/api/updateUserQuizStatistics</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateUserExperiment</servlet-name>
		<url-pattern>/api/updateUserExperiment</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteQuiz</servlet-name>
		<url-pattern>/api/deleteQuiz</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FacebookLogin</servlet-name>
		<url-pattern>/fblogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CampaignManagement</servlet-name>
		<url-pattern>/campaignManagement</url-pattern>
	</servlet-mapping>

	<servlet>
        <servlet-name>appstats</servlet-name>
        <servlet-class>com.google.appengine.tools.appstats.AppstatsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>appstats</servlet-name>
        <url-pattern>/appstats/*</url-pattern>
    </servlet-mapping>

    <security-constraint>
        <web-resource-collection>
            <url-pattern>/appstats/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>
	
	<filter>
        <filter-name>appstats</filter-name>
        <filter-class>com.google.appengine.tools.appstats.AppstatsFilter</filter-class>
        <init-param>
            <param-name>logMessage</param-name>
            <param-value>Appstats available: /appstats/details?time={ID}</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>appstats</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
	   <param-value>
	    us.quizz.endpoints.SurvivalProbabilityEndpoint,
	    us.quizz.endpoints.UtilEndpoint,
		us.quizz.endpoints.QuizEndpoint,
		us.quizz.endpoints.QuestionEndpoint,
		us.quizz.endpoints.QuizPerformanceEndpoint,
		us.quizz.endpoints.UserAnswerEndpoint,
		us.quizz.endpoints.UserAnswerFeedbackEndpoint,
		us.quizz.endpoints.UserEndpoint,
		us.quizz.endpoints.UserReferalEndpoint
	  </param-value>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>
