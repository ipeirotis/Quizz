<queue-entries>
	<total-storage-limit>50M</total-storage-limit>
	<queue>
		<name>default</name>
		<rate>10/s</rate>
		<retry-parameters>
			<task-retry-limit>0</task-retry-limit>
			<task-age-limit>1h</task-age-limit>
		</retry-parameters>
	</queue>

	<queue>
		<name>quizquestions</name>
		<rate>10/s</rate>
		<bucket-size>10</bucket-size>
		<max-concurrent-requests>10</max-concurrent-requests>
		<retry-parameters>
			<task-retry-limit>0</task-retry-limit>
			<task-age-limit>1h</task-age-limit>
		</retry-parameters>
	</queue>

	<queue>
		<name>answers</name>
		<rate>10/s</rate>
		<bucket-size>10</bucket-size>
		<max-concurrent-requests>10</max-concurrent-requests>
		<retry-parameters>
			<task-retry-limit>0</task-retry-limit>
			<task-age-limit>1h</task-age-limit>
		</retry-parameters>
	</queue>

	<queue>
		<name>updateUserStatistics</name>
		<rate>10/s</rate>
		<bucket-size>10</bucket-size>
		<max-concurrent-requests>10</max-concurrent-requests>
		<retry-parameters>
			<task-retry-limit>0</task-retry-limit>
			<task-age-limit>1h</task-age-limit>
		</retry-parameters>
		<target>quizz-tasks</target>
	</queue>

	<queue>
		<name>adgroup</name>
		<rate>1/s</rate>
		<bucket-size>2</bucket-size>
		<max-concurrent-requests>2</max-concurrent-requests>
	</queue>

	<queue>
		<name>adcampaign</name>
		<rate>1/s</rate>
		<bucket-size>1</bucket-size>
		<max-concurrent-requests>1</max-concurrent-requests>
	</queue>

	<queue>
		<name>survival</name>
		<rate>1/s</rate>
		<bucket-size>1</bucket-size>
		<max-concurrent-requests>1</max-concurrent-requests>
	</queue>
</queue-entries>
