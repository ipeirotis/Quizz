<queue-entries>
  <total-storage-limit>500M</total-storage-limit>
  <queue>
    <name>default</name>
    <rate>10/s</rate>
    <bucket-size>10</bucket-size>
    <retry-parameters>
      <task-retry-limit>1</task-retry-limit>
    </retry-parameters>
  </queue>

  <queue>
    <name>userStatistics</name>
    <rate>50/s</rate>
    <bucket-size>50</bucket-size>
    <max-concurrent-requests>100</max-concurrent-requests>
    <target>quizz-tasks</target>
    <retry-parameters>
      <task-retry-limit>0</task-retry-limit>
    </retry-parameters>
  </queue>

  <queue>
    <name>questionStatistics</name>
    <rate>50/s</rate>
    <bucket-size>50</bucket-size>
    <max-concurrent-requests>50</max-concurrent-requests>
    <target>quizz-tasks</target>
    <retry-parameters>
      <task-retry-limit>0</task-retry-limit>
    </retry-parameters>
  </queue>

  <queue>
    <name>adgroup</name>
    <rate>5/s</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
  </queue>

  <queue>
    <name>adcampaign</name>
    <rate>5/s</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
  </queue>

  <queue>
    <name>survival</name>
    <rate>5/s</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
  </queue>

  <queue>
    <name>consistency</name>
    <rate>10/s</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
  </queue>
</queue-entries>
